    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.3",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-env": "^7.0.0",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
